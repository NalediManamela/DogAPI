<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageViewDog"
        android:layout_width="263dp"
        android:layout_height="263dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageButton
        android:id="@+id/btnVoting"
        android:layout_width="55dp"
        android:layout_height="53dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.205"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.191"
        tools:srcCompat="@drawable/upanddown" />

    <ImageButton
        android:id="@+id/btnBreed"
        android:layout_width="47dp"
        android:layout_height="50dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.473"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.188"
        tools:ignore="DuplicateIds"
        tools:srcCompat="@drawable/search" />

    <ImageButton
        android:id="@+id/btnFavs"
        android:layout_width="50dp"
        android:layout_height="54dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.76"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.188"
        tools:srcCompat="@drawable/heart" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="61dp"
        android:layout_height="17dp"
        android:text="Voting"
        tools:layout_editor_absoluteX="73dp"
        tools:layout_editor_absoluteY="194dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Breeds"
        tools:layout_editor_absoluteX="176dp"
        tools:layout_editor_absoluteY="192dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fav"
        tools:layout_editor_absoluteX="290dp"
        tools:layout_editor_absoluteY="192dp" />

    <ImageButton
        android:id="@+id/btnLike"
        android:layout_width="55dp"
        android:layout_height="49dp"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/like"
        tools:layout_editor_absoluteX="243dp"
        tools:layout_editor_absoluteY="548dp" />

    <ImageButton
        android:id="@+id/btnDislike"
        android:layout_width="50dp"
        android:layout_height="52dp"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/dislike"
        tools:layout_editor_absoluteX="298dp"
        tools:layout_editor_absoluteY="548dp" />

    <ImageButton
        android:id="@+id/btnFav2"
        android:layout_width="52dp"
        android:layout_height="56dp"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/heart"
        tools:layout_editor_absoluteX="64dp"
        tools:layout_editor_absoluteY="541dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="20dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="20dp" />
</androidx.constraintlayout.widget.ConstraintLayout>
<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageViewDog"
        android:layout_width="263dp"
        android:layout_height="261dp"
        android:layout_marginStart="75dp"
        android:layout_marginTop="49dp"
        android:layout_marginEnd="73dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="61dp"
        android:layout_height="19dp"
        android:layout_marginTop="18dp"
        android:layout_marginEnd="67dp"
        android:text="Voting"
        app:layout_constraintEnd_toStartOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/btnVoting" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="113dp"
        android:layout_marginBottom="47dp"
        android:text="Breeds"
        app:layout_constraintBottom_toTopOf="@+id/imageViewDog"
        app:layout_constraintEnd_toStartOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/btnBreed" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="177dp"
        android:layout_marginEnd="61dp"
        android:layout_marginBottom="49dp"
        android:text="Fav"
        app:layout_constraintBottom_toTopOf="@+id/imageViewDog"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/btnLike"
        android:layout_width="65dp"
        android:layout_height="57dp"
        android:layout_marginTop="59dp"
        android:layout_marginEnd="25dp"
        android:layout_marginBottom="109dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/btnDislike"
        app:layout_constraintTop_toBottomOf="@+id/imageViewDog"
        app:srcCompat="@drawable/like" />

    <ImageButton
        android:id="@+id/btnDislike"
        android:layout_width="70dp"
        android:layout_height="57dp"
        android:layout_marginTop="59dp"
        android:layout_marginEnd="35dp"
        android:layout_marginBottom="109dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageViewDog"
        app:srcCompat="@drawable/dislike" />

    <ImageButton
        android:id="@+id/btnFav2"
        android:layout_width="75dp"
        android:layout_height="54dp"
        android:layout_marginTop="59dp"
        android:layout_marginEnd="118dp"
        android:layout_marginBottom="112dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/btnLike"
        app:layout_constraintTop_toBottomOf="@+id/imageViewDog"
        app:srcCompat="@drawable/heart" />

    <ImageButton
        android:id="@+id/btnVoting"
        android:layout_width="51dp"
        android:layout_height="45dp"
        android:layout_marginTop="114dp"
        android:layout_marginEnd="69dp"
        android:layout_marginBottom="18dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintEnd_toStartOf="@+id/btnBreed"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/upanddown" />

    <ImageButton
        android:id="@+id/btnBreed"
        android:layout_width="53dp"
        android:layout_height="50dp"
        android:layout_marginTop="114dp"
        android:layout_marginEnd="90dp"
        android:layout_marginBottom="18dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintEnd_toStartOf="@+id/btnFavs"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/search" />

    <ImageButton
        android:id="@+id/btnFavs"
        android:layout_width="56dp"
        android:layout_height="42dp"
        android:layout_marginTop="114dp"
        android:layout_marginEnd="49dp"
        android:layout_marginBottom="18dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/heart" />

</androidx.constraintlayout.widget.ConstraintLayout>
<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity3">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="48dp"
        android:layout_height="19dp"
        android:text="Fav"
        tools:layout_editor_absoluteX="269dp"
        tools:layout_editor_absoluteY="190dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Breeds"
        tools:layout_editor_absoluteX="157dp"
        tools:layout_editor_absoluteY="190dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="61dp"
        android:layout_height="19dp"
        android:text="Voting"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="190dp" />

    <ImageButton
        android:id="@+id/btnFavs"
        android:layout_width="50dp"
        android:layout_height="54dp"
        android:layout_marginStart="59dp"
        android:layout_marginTop="120dp"
        android:layout_marginEnd="94dp"
        android:layout_marginBottom="16dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btnBreed"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@drawable/heart" />

    <ImageButton
        android:id="@+id/btnBreed"
        android:layout_width="51dp"
        android:layout_height="51dp"
        android:layout_marginTop="120dp"
        android:layout_marginBottom="19dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.436"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="DuplicateIds"
        tools:srcCompat="@drawable/search" />

    <ImageButton
        android:id="@+id/btnVoting"
        android:layout_width="55dp"
        android:layout_height="53dp"
        android:layout_marginStart="51dp"
        android:layout_marginTop="118dp"
        android:layout_marginEnd="51dp"
        android:layout_marginBottom="19dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintEnd_toStartOf="@+id/btnBreed"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@drawable/upanddown" />
</androidx.constraintlayout.widget.ConstraintLayout>